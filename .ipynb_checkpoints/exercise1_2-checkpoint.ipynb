from matplotlib import pyplot as plt
import numpy as np

# Let's use numpy to create a list that display fifteen zeroes's. 1
data = np.zeros(15)
plt.show(data) 

# Let's use numpy to create a list that display fifteen one's. 2
data1 = np.ones(15)
print(data1)

# Let's use numpy to create a list that display fifteen sevens's. 3
data2 = np.full(15, 7)
print(data2)

# Let's use numpy to create array of numbers from 100 to 150.
data3 = np.arange(100, 151)
print(data3)

# Let's use numpy to create array of number from 1900 to 2021.
data4 = np.arange(1900, 2022)
print(data4)

# let's use numpy to create array of even numbers between 0 and 100.
data5 = np.arange(0, 101, 2)
print(data5)

# Let's use numpy to create array of years that are divisible by 4 between 1950 and 2020.
data6 = np.arange(1952, 2021, 4)
print(data6)

